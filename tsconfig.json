{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "dom",
            "es6"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "resolveJsonModule": false
    },

    "exclude": [
        
        ".vscode-test",
        "out/**/*"
    ]
}